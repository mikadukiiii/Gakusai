<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#" 
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
	<owl:Ontology rdf:about="">
		<owl:imports rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl"/>
	</owl:Ontology>
	<rdf:Description rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Scenario3">
		<rdf:type>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Activity"/>
					<owl:Restriction>
						<owl:onProperty rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#isToEat"/>
							<owl:allValuesFrom>
								<owl:Class>
									<owl:intersectionOf rdf:parseType="Collection">
										<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Burger"/>
										<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Pizza"/>
										<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Salad"/>
									</owl:intersectionOf>
								</owl:Class>
							</owl:allValuesFrom>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#isToDrink"/>
							<owl:allValuesFrom>
								<owl:Class>
									<owl:intersectionOf rdf:parseType="Collection">
										<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Coke"/>
										<owl:Class rdf:about="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#Water"/>
									</owl:intersectionOf>
								</owl:Class>
							</owl:allValuesFrom>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#occursIn"/>
						<owl:allValuesFrom rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#FastFood"/>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="http://sisinflab.poliba.it/ieva/PhysicalWeb/shopping.owl#occursIn"/>
						<owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</rdf:type>
	</rdf:Description>
</rdf:RDF>
